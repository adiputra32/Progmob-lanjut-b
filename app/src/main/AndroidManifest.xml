<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.adiputra.sewainbali">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">
        <activity
            android:name="com.example.adiputra.sewainbali.DetailHistoryActivity"
            android:label="Detail History"/>
        <activity
            android:name="com.example.adiputra.sewainbali.LandingSuccessActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name="com.example.adiputra.sewainbali.CheckOutActivity"
            android:label="Checkout"/>
        <activity
            android:name="com.example.adiputra.sewainbali.DetailMotorActivity"
            android:label="Detail Motor"/>
        <activity
            android:name="com.example.adiputra.sewainbali.SearchMotorActivity"
            android:label="Search Bike" />
        <activity
            android:name="com.example.adiputra.sewainbali.ChatRoomActivity"
            android:label="Chat Room" />
        <activity
            android:name="com.example.adiputra.sewainbali.MainActivity"
            android:label="@string/title_activity_main2" />
        <activity
            android:name="com.example.adiputra.sewainbali.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.adiputra.sewainbali.EditAccountActivity"
            android:label="Edit Profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.adiputra.sewainbali.SingleViewActivity" />
        <activity
            android:name="com.example.adiputra.sewainbali.ChangePasswordActivity"
            android:label="Change Password" />
    </application>

</manifest>